{
  "name": "hackart",
  "version": "0.0.1",
  "scripts": {
    "lint": "eslint . --ext .js",
    "start": "NODE_ENV=production bash -c 'node server'",
    "start-dev": "node server",
    "build": "rimraf dist/* && rimraf server/*.bundle.js && NODE_ENV=production bash -c 'webpack --progress --colors'",
    "build-dev": "rimraf dist/* && rimraf server/*.bundle.js && webpack --progress --colors --watch --display-error-details",
    "test": "karma start --single-run --browsers PhantomJS && jest",
    "test-dev": "karma start",
    "jest-dev": "node node_modules/.bin/jest --watch"
  },
  "author": "Anshul Nema",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.0",
    "classnames": "2.2.5",
